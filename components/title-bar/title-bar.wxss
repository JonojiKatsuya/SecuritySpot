wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.title-bar {
    background: #fff;
    display: -webkit-flex;
    display: flex;
    height: 180rpx;
    -webkit-justify-content: left;
    justify-content: left;
    left: 0;
    padding: 0 0 0 10rpx;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9
}

.title-bar .icon-item {
    cursor: pointer;
    height: 48rpx;
    margin: 100rpx 8rpx 0 20rpx;
    width: 48rpx
}

