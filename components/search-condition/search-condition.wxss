wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.search-condition-list {
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap
}

.search-condition-item,.search-condition-list {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.search-condition-item {
    border: 1px solid #999;
    border-radius: 30rpx;
    color: #333;
    font-size: 28rpx;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    margin: 12rpx 12rpx 0 0;
    min-width: 82rpx;
    padding: 6rpx 18rpx
}

.icon-close {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 34rpx;
    margin: 0 0 0 16rpx;
    width: 34rpx
}

.search-condition-handle {
    color: #333;
    font-size: 28rpx;
    -webkit-justify-content: flex-end;
    justify-content: flex-end;
    padding: 0 0 10rpx
}

.search-condition-clear,.search-condition-handle {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.icon-clear {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 34rpx;
    width: 34rpx
}

.search-condition-save {
    border: 1px solid #999;
    border-radius: 30rpx;
    margin: 0 0 0 12rpx;
    min-width: 80rpx;
    padding: 4rpx 14rpx
}
