wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.case-tab {
    padding: 16rpx 0 6rpx
}

.case-tab-bar {
    height: 60rpx;
    white-space: nowrap
}

.case-tab-item {
    color: #333;
    display: inline-block;
    font-size: 28rpx;
    margin: 0 45rpx 0 0;
    position: relative;
    text-align: center
}

.case-tab-item-active {
    color: #9b0000
}

.case-tab-item-active::after {
    background-color: #9b0000;
    border-radius: 2rpx;
    bottom: -8rpx;
    content: "";
    height: 4rpx;
    left: 50%;
    margin: 0 0 0 -56rpx;
    position: absolute;
    width: 112rpx
}

.case-tab-content {
    color: #333;
    font-size: 26rpx;
    margin: 8rpx 0 0rpx;
    overflow: hidden;
    position: relative;
    text-align: justify
}

.case-tab-content .mask-panel {
    background-color: initial;
    bottom: 0;
    height: 80rpx;
    left: 0;
    position: absolute;
    width: 100%;
    z-index: 9
}

.case-tab-content-line {
    margin: 8rpx 0
}

.case-tab-content-hei {
    height: 186rpx
}

.case-tab-content-all {
    height: 210rpx
}

.case-tab-more {
    height: 46rpx;
    position: relative
}

.case-more-btn {
    height: 18rpx;
    left: 50%;
    position: absolute;
    top: 20rpx;
    width: 18rpx
}

.down-btn {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.up-btn {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.link-details {
    color: #9b0000;
    font-size: 26rpx;
    text-decoration: underline
}

.cpgz-multi {
    margin: 4rpx 0
}
