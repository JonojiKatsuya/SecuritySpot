wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.popup-wrapper {
    background-color: #fff;
    bottom: 0;
    left: 0;
    position: absolute;
    width: 100%;
    z-index: 9
}

.popup-wrapper .popup-header {
    color: #666;
    font-size: 32rpx;
    padding: 30rpx 0;
    text-align: center
}

.share-panel {
    display: -webkit-flex;
    display: flex
}

.share-panel .share-item {
    background-color: #fff;
    color: #666;
    -webkit-flex: 1;
    flex: 1;
    font-size: 28rpx;
    padding: 0;
    text-align: center
}

.share-panel .share-item:after {
    border: none
}

.share-panel .share-item .share-item-text {
    margin: 14rpx 0
}

.share-panel .share-item-icon {
    height: 100rpx;
    margin: 0 auto;
    width: 100rpx
}

.share-panel .icon-wx {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.share-panel .icon-poster {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.share-panel .icon-link {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.popup-submit {
    background-color: #9b0000;
    color: #fff;
    font-size: 30rpx;
    padding: 20rpx 0
}
