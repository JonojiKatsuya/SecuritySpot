wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.popup-tips {
    background-color: #fff;
    padding: 40rpx 120rpx
}

.popup-tips-icon {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 100rpx;
    margin: 20rpx auto;
    width: 100rpx
}

.popup-tips-content {
    text-align: center
}

.popup-tips-content wx-view {
    margin: 20rpx 0
}

.popup-tips-btn {
    background: #9b0000;
    border-radius: 8rpx;
    color: #fff;
    height: 66rpx;
    line-height: 66rpx;
    margin: 80rpx auto 0;
    text-align: center;
    width: 260rpx
}
