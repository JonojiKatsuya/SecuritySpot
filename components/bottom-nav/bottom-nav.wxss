wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.bottom-nav {
    background: #fff;
    bottom: 0;
    box-shadow: 0 0 5px #d3e2e2;
    display: -webkit-flex;
    display: flex;
    left: 0;
    padding: 22rpx 0;
    position: fixed;
    width: 100%;
    z-index: 2
}

.bottom-nav .bottom-nav-item {
    color: #666;
    -webkit-flex: 1;
    flex: 1;
    font-size: 28rpx;
    text-align: center
}

.bottom-nav .bottom-nav-item .bottom-nav-icon {
    height: 40rpx;
    margin: 0 auto;
    width: 40rpx
}

.bottom-nav .bottom-nav-item .icon-b-catalog {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.bottom-nav .bottom-nav-item .icon-b-search {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.bottom-nav .bottom-nav-item .icon-b-share {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}
