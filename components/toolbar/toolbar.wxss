wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.toolbar {
    bottom: 240rpx;
    padding: 24rpx 16rpx;
    position: fixed;
    right: 0;
    z-index: 2
}

.toolbar .icon-home {
    background: #fff url(data:image/png;base64,) no-repeat 2px 2px;
    background-size: 84% 84%
}

.toolbar .icon-text {
    background: #fff url(data:image/png;base64,) no-repeat 5px 5px;
    background-size: 70% 70%
}

.toolbar .icon-link {
    background: #fff url(data:image/png;base64,) no-repeat 4px 5px;
    background-size: 70% 70%
}

.toolbar .icon-voice {
    background: #fff url(data:image/png;base64,) no-repeat 2px 2px;
    background-size: 84% 84%
}

.toolbar .icon-collect-true {
    background: #fff url(data:image/png;base64,) no-repeat 3px 3px;
    background-size: 80% 80%
}

.toolbar .icon-collect-false {
    background: #fff url(data:image/png;base64,) no-repeat 3px 3px;
    background-size: 80% 80%
}

.toolbar .icon-top {
    background: #fff url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%
}

.toolbar-item {
    border: 1px solid #666;
    border-radius: 50%;
    height: 68rpx;
    margin: 24rpx 0;
    width: 68rpx
}
