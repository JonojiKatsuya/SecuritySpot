wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.mfd {
    background-color: #fff;
    border-top: 1px solid #999
}

.search {
    color: #333;
    font-size: 24rpx;
    margin: 204rpx 0 24rpx;
    padding: 0 30rpx
}

.search,.search-dropmenu {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.search-dropmenu {
    -webkit-flex: 0 1 80rpx;
    flex: 0 1 80rpx
}

.icon-catalog {
    height: 56rpx;
    width: 56rpx
}

.search-bar {
    -webkit-flex: 1;
    flex: 1;
    margin: 0 20rpx 0 0;
    position: relative
}

.search-bar wx-input {
    background: #eee;
    border-radius: 30rpx;
    font-size: 26rpx;
    height: 66rpx;
    line-height: 66rpx;
    padding: 0 20rpx 0 60rpx
}

.icon-search {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 36rpx;
    left: 18rpx;
    margin: -18rpx 0 0;
    position: absolute;
    top: 50%;
    width: 36rpx;
    z-index: 2
}

.search-screen {
    -webkit-flex: 0 1 80rpx;
    flex: 0 1 80rpx;
    font-size: 28rpx;
    line-height: 66rpx
}

.search-screen,.tab-panel {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.tab-panel {
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-justify-content: flex-start;
    justify-content: flex-start;
    padding: 20rpx 30rpx
}

.mfd-list,.tab-panel {
    border-top: 2px solid #bac5c7
}

.list-item {
    border-bottom: 1px solid #d8d7d7;
    padding: 24rpx 30rpx 16rpx;
    position: relative
}

.list-item-title {
    color: #333;
    font-size: 28rpx;
    font-weight: 700
}

.list-item-content {
    color: #333;
    font-size: 24rpx;
    padding: 20rpx 0;
    text-indent: 5rpx
}

.list-item-label {
    -webkit-align-items: center;
    align-items: center;
    color: #333;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
    font-size: 24rpx
}

.label-item {
    border: 1px solid #999;
    border-radius: 30rpx;
    margin: 16rpx 16rpx 0 0;
    padding: 6rpx 26rpx
}

.list-item-category {
    border-radius: 30rpx;
    font-size: 24rpx;
    padding: 6rpx 44rpx;
    position: absolute;
    right: 36rpx;
    top: 16rpx;
    z-index: 2
}

.category-add {
    background-color: #9b0000;
    color: #fff
}

.category-fix {
    background-color: #d06329;
    color: #fff
}
