wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.list {
    background-color: #fff;
    border-top: 1px solid #ddd;
    height: 100vh;
    position: relative
}

.search-panel {
    border-bottom: 1px solid #ddd;
    margin: 180rpx 0 0;
    padding: 24rpx 30rpx
}

.condition {
    padding: 0 30rpx;
    position: relative
}

.screen-panel {
    background-color: #fff;
    border-bottom: 1px solid #bac5c7;
    height: 480rpx;
    left: 0;
    padding: 0 30rpx;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 3
}

.screen-item-title {
    color: #999;
    font-size: 28rpx
}

.cause-option {
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
    margin: 18rpx 0
}

.cause-option,.screen-picker {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: flex-start;
    justify-content: flex-start
}

.screen-picker {
    color: #999;
    font-size: 28rpx;
    margin: 32rpx 0
}

.screen-picker > wx-view {
    background-color: #eee;
    border-radius: 30rpx;
    color: #999;
    font-size: 28rpx;
    margin: 0;
    padding: 14rpx 50rpx
}

.screen-picker-active wx-button {
    background-color: #9b0000;
    color: #fff
}

.screen-picker wx-picker {
    background-color: #eee;
    border: 0!important;
    border-radius: 30rpx;
    margin: 0 46rpx 0 0;
    padding: 14rpx 50rpx
}

.screen-btns {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: center;
    justify-content: center;
    margin: 44rpx 0 70rpx
}

.screen-btns wx-view {
    background-color: #eee;
    border-radius: 30rpx;
    color: #999;
    font-size: 28rpx;
    line-height: 20rpx;
    margin: 0 50rpx 0 0;
    padding: 24rpx 60rpx
}

.nav {
    border-top: 1px solid #ddd;
    padding: 12rpx
}

.nav-tab {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    margin: 0 0 18rpx
}

.nav-tab-item {
    background-color: #eee;
    color: #999;
    font-size: 30rpx;
    padding: 16rpx 0;
    text-align: center;
    width: 50%
}

.nav-tab-item-active {
    background-color: #9b0000;
    color: #fff
}

.picker-panel {
    border-top: 1px solid #ddd;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 30rpx;
    white-space: nowrap
}

.picker-item {
    color: #169bd5;
    display: inline-block;
    font-size: 28rpx;
    margin: 0 45rpx 0 0;
    position: relative;
    text-align: center
}

.picker-mask {
    height: 60rpx;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%
}

.list-box {
    border-top: 1px solid #ddd
}

.list-item {
    border-bottom: 8rpx solid #eee
}

.list-item-baseInfo {
    border-bottom: 1px solid #d5d5d5;
    position: relative
}

.list-item-title {
    -webkit-align-items: center;
    align-items: center;
    color: #333;
    display: -webkit-flex;
    display: flex;
    font-size: 30rpx;
    font-weight: 700;
    margin: 12rpx 0 6rpx;
    padding: 0 30rpx
}

.title-index {
    color: #333;
    font-size: 28rpx;
    margin: 0 14rpx 0 0;
    padding: 6rpx;
    text-align: center
}

.list-item-discribe {
    color: #999;
    font-size: 28rpx;
    padding: 0 30rpx 0 56rpx
}

.list-item-hit {
    -webkit-align-items: center;
    align-items: center;
    border-top: 1px solid #d5d5d5;
    color: #9b0000;
    display: -webkit-flex;
    display: flex;
    font-size: 28rpx;
    margin: 12rpx 0 0;
    padding: 14rpx 30rpx
}

.hit-content {
    color: #333
}

.list-item-collect {
    -webkit-align-items: center;
    align-items: center;
    color: #9b0000;
    display: -webkit-flex;
    display: flex;
    font-size: 28rpx;
    position: absolute;
    right: 26rpx;
    top: 16rpx;
    z-index: 2
}

.icon-collect {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 40rpx;
    margin: 0 14rpx 0 0;
    width: 40rpx
}

.list-item-result {
    padding: 0 30rpx
}

.list-item-vip {
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 8rpx solid #eee;
    color: #999;
    display: -webkit-flex;
    display: flex;
    font-size: 28rpx;
    font-style: italic;
    padding: 18rpx 30rpx
}

.list-item-vip wx-text {
    -webkit-flex: 1;
    flex: 1
}

.icon-vip {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 40rpx;
    margin: 0 18rpx 0 0;
    width: 40rpx
}

.submit-vip {
    background-color: #9b0000;
    border-radius: 30px;
    color: #fff;
    font-size: 28rpx;
    letter-spacing: 2px
}

.list-msg {
    font-size: 28rpx;
    padding: 24rpx 30rpx
}

.tips-noresult {
    padding: 20rpx 30rpx
}

.tips-noresult wx-text {
    color: #333;
    display: block;
    font-size: 28rpx;
    margin: 16rpx 0
}

.tips-nomore {
    color: #777;
    font-size: 15px;
    padding: 20rpx 30rpx;
    text-align: center
}
