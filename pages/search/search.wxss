wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.search-panel {
    margin: 180rpx 0 0
}

.search-panel,.search-params {
    padding: 24rpx 30rpx
}

.search-history {
    margin: 0 0 20rpx
}

.search-content-title {
    font-weight: bolder;
    height: 24rpx;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    margin: 0 0 26rpx
}

.search-content-title,.search-content-title .search-content-title-l {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.search-content-title .search-content-title-l {
    font-size: 30rpx
}

.search-content-title .search-content-title-l wx-text {
    color: #666;
    font-size: 24rpx;
    font-weight: 400;
    margin: 0 16rpx
}

.search-content-title .search-content-title-l .icon-more {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 13rpx;
    width: 20rpx
}

.search-content-title .search-content-title-l .icon-more-open {
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
    -webkit-transform-origin: center;
    transform-origin: center;
    transition: .2s
}

.search-content-title .search-content-title-l .icon-more-close {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transform-origin: center;
    transform-origin: center;
    transition: .2s
}

.search-content-title .icon-del {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 40rpx;
    width: 40rpx
}

.search-content-list {
    color: #333;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap
}

.search-content-list,.search-content-list .search-content-item {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.search-content-list .search-content-item {
    font-size: 30rpx;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    margin: 0 0 20rpx;
    width: 50%
}

.search-history .search-content-title .search-content-title-r wx-text {
    font-size: 28rpx;
    margin: 0 0 0 24rpx
}

.search-history .search-content-item .icon-closed {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    height: 20rpx;
    margin: 0 24rpx 0 0;
    width: 20rpx
}
