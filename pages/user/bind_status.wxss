wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.bind_account .bind_account_wrap {
    margin: 180rpx 0 0;
    padding: 200rpx 80rpx
}

.bind_account wx-radio {
    -webkit-transform: scale(1.2);
    transform: scale(1.2)
}

.bind_account .bind_ok,.bind_account .bind_waiting {
    text-align: center
}

.bind_account .bind_name {
    margin: 40rpx 0 60rpx
}

.bind_account .bind_img {
    background: url(data:image/png;base64,) no-repeat;
    background-size: cover;
    height: 142px;
    margin: 0 auto 40rpx;
    width: 200px
}

.bind_account .bind_info_wt {
    padding: 10rpx 0
}

.bind_account .close_bind {
    margin-top: 60rpx
}
