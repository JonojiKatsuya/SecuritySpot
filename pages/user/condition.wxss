.user-condition {
    border-top: 1px solid #999
}

.condition-list {
    margin: 180rpx 0 0
}

.condition-list-item {
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 1px solid #eee;
    display: -webkit-flex;
    display: flex;
    font-size: 28rpx;
    padding: 20rpx 30rpx
}

.condition-item-title {
    color: #333
}

.condition-item-content {
    color: #666;
    margin: 12rpx 0
}

.condition-item-msg {
    -webkit-flex: 1;
    flex: 1
}

.condition-item-status {
    text-align: center
}

.icon-del {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.icon-del,.icon-pc {
    height: 60rpx;
    margin: 0 auto;
    width: 60rpx
}

.icon-pc {
    background: url(data:image/png;base64,);
    background-size: 100% 100%
}

.tips-nomore {
    color: #777;
    font-size: 15px;
    padding: 20rpx 30rpx;
    text-align: center
}
