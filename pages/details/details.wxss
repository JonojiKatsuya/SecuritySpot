}

.detail.font_size2 .field_lable,.detail.font_size2 .structured_info_title {
    font-size: 36rpx
}

.detail.font_size2 .art_fulltext,.detail.font_size2 .field_info,.detail.font_size2 .field_lable_subtitle,.detail.font_size2 .structured_info_content {
    font-size: 32rpx
}

.detail.font_size3 .title {
    font-size: 44rpx
}

.detail.font_size3 .sub_info {
    font-size: 36rpx
}

.detail.font_size3 .field_lable,.detail.font_size3 .structured_info_title {
    font-size: 40rpx
}

.detail.font_size3 .art_fulltext,.detail.font_size3 .field_info,.detail.font_size3 .field_lable_subtitle,.detail.font_size3 .structured_info_content {
    font-size: 36rpx
}

.detail .members_wrapper {
    background-color: #fff;
    border-radius: 5px;
    padding: 20rpx 30rpx;
    width: 200px
}

.detail .members_wrapper .members_header {
    color: #f90;
    text-align: center
}

.detail .members_wrapper .members_page {
    color: #333;
    font-size: 28rpx;
    padding: 20rpx 0;
    text-align: center
}

.detail .members_wrapper .members_footer {
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between
}

.detail .members_wrapper .members_footer wx-button.default {
    -webkit-flex: 1;
    flex: 1;
    margin: 0 10rpx
}

.detail .art_fulltext {
    color: #333;
    font-size: 24rpx
}

.icon-item {
    height: 40rpx;
    margin: 0 24rpx;
    width: 40rpx
}

.icon-text {
    background: url(data:image/png;base64,) no-repeat 0 0;
