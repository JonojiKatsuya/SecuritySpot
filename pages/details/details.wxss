    font-size: 36rpx
}

.detail.font_size1 .sub_info {
    font-size: 28rpx
}

.detail.font_size1 .field_lable,.detail.font_size1 .structured_info_title {
    font-size: 32rpx
}

.detail.font_size1 .art_fulltext,.detail.font_size1 .field_info,.detail.font_size1 .field_lable_subtitle,.detail.font_size1 .structured_info_content {
    font-size: 28rpx
}

.detail.font_size2 .title {
