wx-button.base {
    background: #9b0000;
    color: #fff;
    line-height: 74rpx
}

wx-button.base:active {
    background: #820404;
    color: #fff
}

wx-button.default {
    background: #fff;
    color: #333;
    font-size: 28rpx;
    line-height: 60rpx
}

wx-button.default:active {
    background: #f8f8f8
}

wx-input .input-placeholder {
    color: #666!important;
    font-size: 24rpx
}

wx-uni-radio .uni-radio-input.uni-radio-input-checked {
    background: #9b0000!important;
    border-color: #9b0000!important
}

.detail .detail_scroll {
    border-top: 1px solid #dfdfdf;
    height: calc(100% - 80rpx - 180rpx);
    margin-bottom: 120rpx
}

.detail .detail_scroll .wrapper {
    padding: 0 36rpx 36px;
    position: relative
}

.detail .title {
    -webkit-align-items: flex-start;
    align-items: flex-start;
    display: -webkit-flex;
    display: flex;
    font-size: 30rpx;
    margin: 10rpx 0 20rpx;
    text-align: center
}

.detail .title wx-text {
    -webkit-flex: 1;
    flex: 1;
    margin: 0 30rpx
}

.detail .sub_info {
    -webkit-align-items: center;
    align-items: center;
    color: #333;
    display: -webkit-flex;
    display: flex;
    font-size: 24rpx;
    -webkit-justify-content: center;
    justify-content: center;
    padding: 10rpx 0
}

.detail .sub_info .r {
    padding: 0 10rpx
}

.detail .sub_info .r .icon {
    margin-top: 4rpx;
    vertical-align: top
}

.detail .sub_info .r .read {
    cursor: pointer
}

.detail .noMembers {
    margin: 30rpx auto;
    width: 300rpx
}

.detail .fix-panel {
    top: 180rpx
}

.detail .fix-panel,.detail .fix-panel-bottom {
    background-color: #fff;
    left: 0;
    padding: 0 0 12rpx;
    position: fixed;
    width: 100%;
    z-index: 4
}

.detail .fix-panel-bottom {
    bottom: 0
}

.detail .icon-catalog {
    height: 56rpx;
    width: 56rpx
}

.detail .art_model,.detail .field {
    text-align: justify
}

.detail .field_lable,.detail .structured_info_title {
    color: #333;
    font-size: 24rpx;
    font-weight: 600;
    margin: 8rpx 0
}

.detail .field_lable_subtitle {
    color: #333;
    font-size: 24rpx;
    margin: 8rpx 0
}

.detail .field_info {
    margin: 8rpx 0
}

.detail .field_info,.detail .structured_info_content {
    color: #333;
    font-size: 24rpx;
    line-height: 1.5em;
    white-space: pre-wrap
}

.detail .collection_header {
    background: #efefef;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    line-height: 40px;
    padding: 0 30rpx
}

.detail .collection_header .btn_text {
    color: red
}

.detail .coll_wrapper .uni-list,.detail .uni-popup .uni-list {
    display: block;
    height: 300px;
    overflow: scroll
}

.detail .detail_side_pt {
    bottom: 80px;
    padding: 10rpx;
    position: fixed;
    right: 0
}

.detail .detail_footer_pt {
    background: #f8f8f8;
    bottom: 0;
    left: 0;
    padding: 0 0 40rpx;
    position: fixed;
    width: 100%
}

.detail .detail_footer_pt .footer_tabbar {
    color: #666;
    display: -webkit-flex;
    display: flex;
    font-size: 24rpx;
    height: 48px;
    -webkit-justify-content: space-around;
    justify-content: space-around;
    line-height: 48px
}

.detail .detail_footer_pt .footer_tabbar wx-view {
    -webkit-flex: 1;
    flex: 1;
    text-align: center
}

.detail .detail_footer_pt .font_size {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
    justify-content: space-around;
    padding: 20rpx 50px;
    position: relative
}

.detail .detail_footer_pt .font_size wx-icon {
    width: 26px
}

.detail .detail_footer_pt .font_size wx-slider {
    -webkit-flex: 1;
    flex: 1;
    margin: 10px
}

.detail .detail_footer_pt .side_bar {
    bottom: 100%;
    padding: 10rpx;
    position: absolute;
    right: 0
}

.detail .cata_wrapper {
    margin: 180rpx 0 0;
    padding: 20rpx
}

.detail .cata_wrapper .cata_list {
    color: #333;
    font-size: 28rpx;
    padding: 10rpx 0 0 20rpx
}

.detail .cata_wrapper .cata_list .uni-title {
    line-height: 56rpx;
    padding-left: 16px;
    position: relative
}

.detail .cata_wrapper .cata_list .uni-title:last-child:before {
    content: none
}

.detail .cata_wrapper .cata_list .uni-title:after {
    background-color: #2b85e4;
    border-radius: 50%;
    content: "";
    left: 0;
    margin-top: -6rpx;
    padding: 6rpx;
    position: absolute;
    top: 50%
}

.detail .cata_wrapper .cata_list .uni-title:before {
    border-left: 1px solid #666;
    content: "";
    height: 100%;
    left: 5.5rpx;
    margin-top: -6rpx;
    position: absolute;
    top: 50%
}

